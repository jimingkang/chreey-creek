{
  "version": "0.2.0",
  "configurations": [

{
      "name": "Debug tinycc",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/mytinyCC/build/tinycc",
      "args": ["test.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/miniCC/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "启用 pretty print",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Debug calculator",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/miniCC/build/calculator",
      "args": ["test.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/miniCC/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "启用 pretty print",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
